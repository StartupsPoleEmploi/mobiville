{
  "name": "mobiville-api",
  "version": "0.0.1",
  "license": "AGPL",
  "engines": {
    "node": "16.16.0"
  },
  "scripts": {
    "start": "nodemon --exec babel-node src/index.koa.js",
    "start:production": "node --optimize_for_size --max_old_space_size=2000 --gc_interval=100 dist/index.koa.js",
    "build:production": "npm run _rm-build-prod && npm run _build-prod",
    "_build-prod": "babel src --out-dir dist --copy-files",
    "_rm-build-prod": "rm -rf dist",
    "sync:cities": "babel-node ./src/scripts/syncCities.js",
    "sync:regions": "babel-node ./src/scripts/syncRegions.js",
    "sync:professionsInTension": "babel-node ./src/scripts/syncProfessionsInTension.js",
    "sync:romeOgrs": "babel-node ./src/scripts/syncRomeOgrs.js",
    "sync:romeSkills": "babel-node ./src/scripts/syncRomeSkills.js",
    "sync:tensionsPCS": "babel-node ./src/scripts/syncTensionsPCS.js",
    "sync:regionsTensionsCriterions": "babel-node ./src/scripts/syncRegionsTensionsCriterions.js",
    "sync:equipments": "babel-node ./src/scripts/syncEquipments.js",
    "sync:equipmentsSpecial": "babel-node ./src/scripts/syncSpecialEquipments.js",
    "sync:helps": "babel-node ./src/scripts/syncHelps.js"
  },
  "dependencies": {
    "@koa/cors": "3.3.0",
    "@koa/router": "12.0.0",
    "@sentry/node": "7.7.0",
    "@sentry/tracing": "7.7.0",
    "async": "3.2.4",
    "axios": "0.27.2",
    "axios-cache-adapter": "^2.7.3",
    "bcryptjs": "2.4.3",
    "bz2": "1.0.1",
    "cheerio": "1.0.0-rc.12",
    "config": "3.3.7",
    "cron": "2.1.0",
    "crypto": "1.0.1",
    "csv-parse": "5.3.0",
    "dotenv": "16.0.1",
    "fs": "0.0.1-security",
    "html-entities": "2.3.3",
    "jsonwebtoken": "8.5.1",
    "koa": "2.13.4",
    "koa-body": "5.0.0",
    "koa-compress": "5.1.0",
    "koa-helmet": "6.1.0",
    "koa-session": "6.2.0",
    "koa2-ratelimit": "1.1.1",
    "lodash": "4.17.21",
    "mariadb": "3.0.0",
    "moment": "2.29.4",
    "node-gzip": "1.1.2",
    "path": "0.12.7",
    "querystring": "0.2.1",
    "sequelize": "6.21.3",
    "sequelize-cli": "6.4.1",
    "sequelize-transforms": "2.0.0",
    "slugify": "1.6.5",
    "string-strip-html": "11.0.0",
    "winston": "3.8.1",
    "xml2json": "0.12.0"
  },
  "resolutions":{
    "minimist": "1.2.6",
    "ini":"1.3.6",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "ansi-regex":"5.0.1",
    "path-parse":"1.0.7",
    "color-string":"1.5.5"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-3": "6.17.0",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "expect": "28.1.3",
    "mocha": "10.0.0",
    "nodemon": "2.0.19",
    "prettier-eslint": "15.0.1"
  }
}
