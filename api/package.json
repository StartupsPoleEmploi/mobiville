    "ansi-regex": "5.0.1",
    "path-parse": "1.0.7",
    "color-string": "1.9.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.21.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "8.7.0",
    "nodemon": "2.0.21",
    "prettier-eslint": "15.0.1"
  }
}
