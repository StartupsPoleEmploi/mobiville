{
  "name": "mobiville-api",
  "version": "0.0.1",
  "engines": {
    "node": "12.18.4"
  },
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "start:production": "node --optimize_for_size --max_old_space_size=2000 --gc_interval=100 dist/index.js",
    "build:production": "npm run _rm-build-prod && npm run _build-prod",
    "_build-prod": "babel src --out-dir dist --copy-files",
    "_rm-build-prod": "rm -rf dist",
    "sync:all": "babel-node ./src/scripts/syncCities.js && babel-node ./src/scripts/syncProfessionsInTension.js && babel-node ./src/scripts/syncRegions.js"
  },
  "dependencies": {
    "async": "^3.2.0",
    "axios": "0.21.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.2",
    "cron": "^1.8.2",
    "crypto": "^1.0.1",
    "csv-parse": "^4.14.0",
    "dotenv": "^8.2.0",
    "fs": "0.0.1-security",
    "html-entities": "^1.3.1",
    "jsonwebtoken": "^8.5.1",
    "koa-body": "^4.0.6",
    "koa-session": "^6.1.0",
    "koa-smart": "^2.2.0",
    "lodash": "^4.17.20",
    "mariadb": "^2.5.1",
    "moment": "^2.29.1",
    "path": "^0.12.7",
    "querystring": "^0.2.0",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-transforms": "^2.0.0",
    "slugify": "^1.4.6",
    "string-strip-html": "^6.3.0",
    "winston": "^3.3.3",
    "xml2json": "^0.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.17.0",
    "eslint": "^7.13.0",
    "expect": "^26.6.2",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "prettier-eslint": "^11.0.0"
  }
}
